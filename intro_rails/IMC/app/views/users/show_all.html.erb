<h1>Users!!!!!!!!</h1>
<table>
    <thead>
        <tr>
            <th>ID:</th>
            <th>First Name: </th>
            <th>Last Name: </th>
            <th>Age: </th>
            <th>Created At:</th>
            <th>Updated At:</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= user.age %></td>
            <td><%= user.created_at %></td>
            <td><%= user.updated_at %></td>
            <td><a href="/users/<%= user.id %>">View</a></td>
            <td><a href="/users/<%= user.id %>/edit">Edit</a></td>
        </tr>
        <% end %>
    </tbody>
</table>